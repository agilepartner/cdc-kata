{
  "consumer": {
    "name": "Alcoholic"
  },
  "provider": {
    "name": "Bar Api"
  },
  "interactions": [
    {
      "description": "A POST request to order a beer",
      "providerState": "A client is adult (age >= 18)",
      "request": {
        "method": "post",
        "path": "/bar/beers",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "Age": 18
        }
      },
      "response": {
        "status": 201,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}